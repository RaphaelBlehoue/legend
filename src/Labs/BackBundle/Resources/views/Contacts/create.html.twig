{% extends 'LabsTemplateBundle::layout.html.twig' %}
{% block Title %}{{ 'Adresse - catégories | Legend Photography' }}{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('bundles/labstemplate/assets/css/admin/module.admin.page.form_elements.min.css') }}" />
{% endblock %}
{% block body %}
    <h1 class="bg-white content-heading border-bottom">Dashboard</h1>
    <div class="bg-white innerAll border-bottom">
        <ul class="menubar">
            <li class="active"><a href="index.html?lang=en">Dashboard</a></li>
            <li><a href="#">{{ 'Categories' }}</a></li>
            <li><a href="{{ path('contact_index') }}">{{ 'Contacts' }}</a></li>
            <li><a href="#">{{ 'Creation' }}</a></li>
        </ul>
    </div>
    <div class="innerAll spacing-x2">
        {{ form_start(form) }}
            <!-- Row -->
            <div class="row">
                <!-- Column -->
                <div class="col-md-10 col-md-offset-1">
                    <!-- Widget -->
                    <div class="widget widget-inverse" >
                        <!-- Widget heading -->
                        <div class="widget-head">
                            <a href="{{ path('contact_index') }}" class="btn btn-xs btn-info pull-right">
                                <i class="fa fa-plus fa-fw"></i> {{ 'Liste des contacts'|upper }}
                            </a>
                            <h4 class="heading">{{ 'Creer contacts'|upper }}</h4>
                        </div>
                        <!-- // Widget heading END -->
                        <div class="widget-body">
                            <div class="innerB">
                                <div class="row">
                                    <div class="col-xs-8 col-xs-offset-2 form-group">
                                        <label class="control-label" for="name">{{ 'Adresse' }}</label>
                                        {{ form_row(form.adresse) }}
                                        {{ form_errors(form.adresse) }}
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-xs-8 col-xs-offset-2 form-group">
                                        <label class="control-label" for="name">{{ 'Phone' }}</label>
                                            {{ form_row(form.phone) }}
                                            {{ form_errors(form.phone) }}
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-xs-8 col-xs-offset-2 form-group">
                                        <label class="control-label" for="name">{{ 'Email' }}</label>
                                        {{ form_row(form.email) }}
                                        {{ form_errors(form.email) }}
                                    </div>
                                </div>


                                <div class="row">
                                    <div class="col-xs-8 col-xs-offset-2 form-group">
                                        <label class="control-label" for="name">{{ 'fax' }}</label>
                                        {{ form_row(form.faxe) }}
                                        {{ form_errors(form.faxe) }}
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-xs-8 col-xs-offset-2 form-group">
                                        <label class="control-label" for="name">{{ 'Facebook' }}</label>
                                        {{ form_row(form.facebook) }}
                                        {{ form_errors(form.facebook) }}
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-xs-8 col-xs-offset-2 form-group">
                                        <label class="control-label" for="name">{{ 'Skype' }}</label>
                                        {{ form_row(form.skype) }}
                                        {{ form_errors(form.skype) }}
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-xs-8 col-xs-offset-2 form-group">
                                        <label class="control-label" for="name">{{ 'Youtube' }}</label>
                                        {{ form_row(form.youtube) }}
                                        {{ form_errors(form.youtube) }}
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-xs-8 col-xs-offset-2 form-group">
                                        <label class="control-label" for="name">{{ 'vimeo' }}</label>
                                        {{ form_row(form.vimeo) }}
                                        {{ form_errors(form.vimeo) }}
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-xs-8 col-xs-offset-2 form-group">
                                        <label class="control-label" for="name">{{ 'pinterest' }}</label>
                                        {{ form_row(form.pinterest) }}
                                        {{ form_errors(form.pinterest) }}
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-xs-8 col-xs-offset-2 form-group">
                                        <label class="control-label" for="name">{{ 'instagram' }}</label>
                                        {{ form_row(form.instagram) }}
                                        {{ form_errors(form.instagram) }}
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-xs-8 col-xs-offset-2 form-group">
                                        <label class="control-label" for="name">{{ 'Google+' }}</label>
                                        {{ form_row(form.google) }}
                                        {{ form_errors(form.google) }}
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xs-8 col-xs-offset-2 form-group">
                                        <label class="control-label" for="name">{{ 'twitter' }}</label>
                                        {{ form_row(form.twitter) }}
                                        {{ form_errors(form.twitter) }}
                                    </div>
                                </div>


                                <div class="row">
                                    <div class="form-group">
                                        <div class="col-sm-offset-2 col-sm-10">
                                            <button type="submit" class="btn btn-success" formnovalidate>{{ 'Créer le contact' }}</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- // Widget END -->
                </div>
                <!-- // Column END -->
            </div>
            <!-- // Row END -->
        {{ form_end(form) }}
    </div>
{% endblock %}

