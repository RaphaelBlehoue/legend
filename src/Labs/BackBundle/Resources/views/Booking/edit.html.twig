{% extends 'LabsTemplateBundle::layout.html.twig' %}
{% block Title %}{{ 'Reservation | Legend Photography' }}{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('bundles/labstemplate/assets/css/admin/module.admin.page.form_elements.min.css') }}" />
{% endblock %}
{% block body %}
    <h1 class="bg-white content-heading border-bottom">Dashboard</h1>
    <div class="bg-white innerAll border-bottom">
        <ul class="menubar">
            <li class="active"><a href="">Dashboard</a></li>
            <li><a href="{{ path('booking_index') }}">{{ 'Reservation' }}</a></li>
            <li><a href="#">{{ 'Edition' }}</a></li>
        </ul>
    </div>
    <div class="innerAll spacing-x2">
        {{ form_start(form) }}
        <!-- Row -->
        <div class="row">
            <!-- Column -->
            <div class="col-md-10 col-md-offset-1">
                <!-- Widget -->
                <div class="widget widget-inverse" >
                    <!-- Widget heading -->
                    <div class="widget-head">
                        <a href="{{ path('booking_index') }}" class="btn btn-xs btn-info pull-right">
                            <i class="fa fa-plus fa-fw"></i> {{ 'Liste des reservations' }}
                        </a>
                        <h4 class="heading">{{ 'Modifier la reservation'|upper }}</h4>
                    </div>
                    <!-- // Widget heading END -->
                    <div class="widget-body">
                        <div class="innerB">
                            <div class="row">
                                <div class="col-xs-8 col-xs-offset-2 form-group">
                                    <label class="control-label" for="name">{{ 'Nom du reservant' }}</label>
                                    {{ form_row(form.name) }}
                                    {{ form_errors(form.name) }}
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xs-8 col-xs-offset-2 form-group">
                                    <label class="control-label" for="name">{{ 'Lieu de l\'evenement' }}</label>
                                    {{ form_row(form.lieu) }}
                                    {{ form_errors(form.lieu) }}
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xs-8 col-xs-offset-2 form-group">
                                    <label class="control-label" for="name">{{ 'email du reservant' }}</label>
                                    {{ form_row(form.email) }}
                                    {{ form_errors(form.email) }}
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xs-8 col-xs-offset-2 form-group">
                                    <label class="control-label" for="name">{{ 'date de reservation' }}</label>
                                    {{ form_row(form.date_res) }}
                                    {{ form_errors(form.date_res) }}
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-8 col-xs-offset-2 form-group">
                                    <label class="control-label" for="name">{{ 'Choix du pack' }}</label>
                                    {{ form_row(form.pack) }}
                                    {{ form_errors(form.pack) }}
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xs-8 col-xs-offset-2 form-group">
                                    <label class="control-label" for="name">{{ 'Valid√© la reservation' }}</label>
                                    {{ form_row(form.status) }}
                                    {{ form_errors(form.status) }}
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group">
                                    <div class="col-sm-offset-2 col-sm-10">
                                        <button type="submit" class="btn btn-primary" formnovalidate>{{ 'Modifier la reservation' }}</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- // Widget END -->
            </div>
            <!-- // Column END -->
        </div>
        <!-- // Row END -->
        {{ form_end(form) }}
    </div>
{% endblock %}

