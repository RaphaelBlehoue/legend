<head>
    <title>{% block Title %}{% endblock %}</title>

    <!-- Meta -->
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7; IE=EDGE" />

    <!--
    **********************************************************
    In development, use the LESS files and the less.js compiler
    instead of the minified CSS loaded by default.
    **********************************************************
    <link rel="stylesheet/less" href="../assets/less/admin/module.admin.page.index.less" />
    -->

    <!--[if lt IE 9]>
    <link rel="stylesheet" href="{{ asset('bundles/labstemplate/assets/components/library/bootstrap/css/bootstrap.min.css') }}" />
    <![endif]-->
    {% block stylesheets %}{% endblock %}
</head>
<body class=" loginWrapper">

{% for type, messages in app.session.flashBag.all %}
    <div class="alert alert-primary">
        <button type="button" class="close" data-dismiss="alert">Ã—</button>
        {% for message in messages %}
                <strong>Warning!</strong> {{ message|trans({}, 'FOSUserBundle') }}
        {% endfor %}
    </div>
{% endfor %}

<div id="content">
    {% block fos_user_content %}{% endblock fos_user_content %}
</div>
    <!-- // Main Container Fluid END -->

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->

    <script src="{{ asset('bundles/labstemplate/assets/components/library/jquery/jquery.min.js?v=v1.2.3') }}"></script>
    <script src="{{ asset('bundles/labstemplate/assets/components/library/jquery/jquery-migrate.min.js?v=v1.2.3') }}"></script>
    <script src="{{ asset('bundles/labstemplate/assets/components/library/modernizr/modernizr.js?v=v1.2.3') }}"></script>
    <script src="{{ asset('bundles/labstemplate/assets/components/plugins/less-js/less.min.js?v=v1.2.3') }}"></script>
    <script src="{{ asset('bundles/labstemplate/assets/components/modules/admin/charts/flot/assets/lib/excanvas.js?v=v1.2.3') }}"></script>
    <script src="{{ asset('bundles/labstemplate/assets/components/plugins/browser/ie/ie.prototype.polyfill.js?v=v1.2.3') }}"></script>
    <script src="{{ asset('bundles/labstemplate/assets/components/library/jquery-ui/js/jquery-ui.min.js?v=v1.2.3') }}"></script>
    <script src="{{ asset('bundles/labstemplate/assets/components/plugins/jquery-ui-touch-punch/jquery.ui.touch-punch.min.js?v=v1.2.3') }}"></script>


    <!-- Global -->
    <script>
        var basePath = '',
                commonPath = '{{ asset('bundles/labstemplate/assets/') }}',
                rootPath = '{{ asset('bundles/labstemplate/') }}',
                DEV = false,
                componentsPath = '{{ asset('bundles/labstemplate/assets/components/') }}';

        var primaryColor = '#cb4040',
                dangerColor = '#b55151',
                infoColor = '#466baf',
                successColor = '#8baf46',
                warningColor = '#ab7a4b',
                inverseColor = '#45484d';

        var themerPrimaryColor = primaryColor;
    </script>

    <script type="text/javascript">
        $(document).ready(function(){
            var selector = '.navParent li';
            $(selector).on('click', function(){
                $(selector).removeClass('active');
                $(this).addClass('active');
            });
        });
    </script>

    {% block javascripts %}{% endblock %}
</body>
</html>